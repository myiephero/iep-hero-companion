{
  "name": "@iep-hero/mobile",
  "private": true,
  "version": "1.0.0",
  "description": "IEP Hero Mobile App - iOS/Android Capacitor Application",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5000",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:build": "npm run build && npx cap sync",
    "cap:build:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "cap:build:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:sync:android": "npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "ios:build:testflight": "./ios/build-scripts/build-testflight.sh",
    "ios:build:dev": "npm run build && npx cap sync ios",
    "ios:build:prod": "npm run build && npx cap sync ios && npm run ios:build:testflight",
    "ios:prepare": "npx cap sync ios && cd ios/App && pod install",
    "android:build:aab": "./scripts/android/build-aab.sh",
    "android:build:aab:debug": "./scripts/android/build-aab.sh --debug",
    "android:build:apk": "./scripts/android/build-apk.sh",
    "android:build:apk:release": "./scripts/android/build-apk.sh --release",
    "android:keystore:generate": "./android/keystore/generate-keystore.sh",
    "android:upload:guide": "./scripts/android/upload-to-play-console.sh",
    "android:build:prod": "npm run build && ./scripts/android/build-aab.sh",
    "android:build:all": "npm run android:build:apk && npm run android:build:aab"
  },
